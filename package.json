{
  "name": "servbuzios",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "generate:component": "yarn plop --plopfile generators/components-plopfile.js",
    "generate:template": "yarn plop --plopfile generators/template-plopfile.js",
    "storybook": "start-storybook ./public -p 6006",
    "build-storybook": "build-storybook ./public"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix"
    ]
  },
  "dependencies": {
    "@emailjs/browser": "^3.10.0",
    "add": "^2.0.6",
    "graphql": "^15.7.1",
    "graphql-request": "^3.6.1",
    "joi": "^17.4.0",
    "next": "12.1.5",
    "next-pwa": "^5.5.2",
    "next-seo": "^5.4.0",
    "nextjs-progressbar": "^0.0.11",
    "polished": "^4.2.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-google-recaptcha": "^2.1.0",
    "react-icons": "^4.3.1",
    "react-pagination-bar": "^2.0.1",
    "react-scroll-to-top": "^3.0.0",
    "react-slick": "^0.29.0",
    "react-toastify": "^9.1.1",
    "styled-components": "5.3.5",
    "styled-media-query": "^2.1.2"
